let obj = {
  currentValue: undefined,
  queue: [],

  add(number) {
    this.currentValue += number;
    return
  },

  mulitply(number) {
    if (number == '') {
      return
    }
    this.currentValue *= number;
    return
  },

  divide(number) {
    this.currentValue /= number;
    return
  }
}


